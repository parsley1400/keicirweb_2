    <div class="icons">
    <% if @post.twitter == nil %>
    <% elsif @post.twitter =~ /(?:\p{Hiragana}|\p{Katakana}|[一-龠々])/ %>
    <% else %>
        <a href="https://twitter.com/<%= @post.twitter %>" target=”_blank”><i class="fab fa-twitter icon"></i></a>
    <% end %>
    <% if @post.instagram == nil %>
    <% elsif @post.instagram.include?('http') %>
