<div class="contents">
    <p class="midashi"><%= @post.name %></p>
    <div class="tags">
        <% if @post.tag_a == nil %>
        <% else %>
        <div class="tag tag1">
            <div class="tagmaru tagmaru1"></div>
            <p><%= @post.tag_a %></p>
        </div>
        <% end %>
        <% if @post.tag_b == nil %>
        <% else %>
